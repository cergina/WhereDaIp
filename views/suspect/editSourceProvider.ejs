<!DOCTYPE html>
<html lang="en">
<!-- head -->
<%- include('../_header') %> 

<body onload="afterLoaded()">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/2.1.2/sweetalert.min.js"></script>
    
    <div class="container">
        <div class="alert alert-info">
            <h1 class="mb-4 display-4">Edit provider</h1>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="/">Home</a></li>
                  <li class="breadcrumb-item"><a href="/suspect">Suspicious</a></li>
                  <li class="breadcrumb-item active" aria-current="page">Edit</li>
                </ol>
              </nav>
        </div>
        <div class="bd-callout bd-callout-info">
            <p>
                Edit provider you've entered previously.
            </p>
        </div>
         
        <form action="/suspect/edit/<%= provider.slug %> " method="POST">
            <!-- this is to output raw html instead of converting to String -->
            <%- include('_form_fields_suspect_source') %>
        </form>

    </div>
</body>


